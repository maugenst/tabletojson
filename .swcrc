{
    "module": {
        "type": "es6"
    },
    "jsc": {
        "parser": {
            "syntax": "typescript",
            "tsx": false,
            "decorators": true,
            "dynamicImport": true
        },
        "transform": {
            "legacyDecorator": true,
            "decoratorMetadata": true
        },
        "target": "es2022",
        "loose": false,
        "minify": {
            "compress": false,
            "mangle": false
        }
    },
    "minify": false,
    "isModule": true,
    "sourceMaps": "inline",
    "inlineSourcesContent": true,
    "exclude": [
        "__mocks__"
    ]
}
