
{
    "transform": {
        "^.+\\.(ts|tsx)$": ["ts-jest", {
            "diagnostics": false,
            "useESM": true
        }]
    },
    "extensionsToTreatAsEsm": [".ts", ".tsx"],
    "testRegex": "/.+test\\.ts",
    "forceExit": true,
    "silent": true,
    "bail": true,
    "testTimeout": 3000000,
    "rootDir": "./"
}
