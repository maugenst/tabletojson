
{
    "name": "Tabletojson",
    "transform": {
        "^.+\\.(ts|tsx)$": "@swc/jest"
    },
    "extensionsToTreatAsEsm": [".ts", ".tsx"],
    "testRegex": "/.+test\\.ts",
    "forceExit": true,
    "silent": true,
    "bail": true,
    "testTimeout": 3000000,
    "rootDir": "./",
    "moduleNameMapper": {
        "^(\\.{1,2}/.*)\\.js$": "$1"
    }
}
